        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/http-client": "^5.3"
    },
    "require-dev": {
        "camelot/coding-style": "^3.0",
        "symfony/phpunit-bridge": "^5.3",
        "friendsofphp/php-cs-fixer": "^3.1",
        "phpunit/phpunit": "^9.5",
        "symfony/dotenv": "^5.3"
    },
    "autoload": {
        "psr-4": {
